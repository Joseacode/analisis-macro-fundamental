{
    "name": "sec-yahoo-backend",
    "version": "1.0.0",
    "type": "module",
    "main": "index.mjs",
    "scripts": {
        "dev:server": "node --watch index.mjs",
        "dev:wrangler": "wrangler dev",
        "start": "node index.mjs",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.21.2",
        "node-cron": "^4.2.1",
        "serpapi": "^2.2.1",
        "yahoo-finance2": "^3.13.0"
    },
    "devDependencies": {
        "jest": "^30.2.0",
        "wrangler": "^3.0.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.(spec|test).(js|cjs|mjs)",
            "**/?(*.)+(spec|test).(js|cjs|mjs)"
        ],
        "collectCoverageFrom": [
            "server/services/**/*.(js|cjs|mjs)",
            "!server/services/**/__tests__/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        }
    }
}